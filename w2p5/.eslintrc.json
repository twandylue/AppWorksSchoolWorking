{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 1,
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        "quotes": ["error", "double"],
        "indent": ["error", 4],
        "semi": [2, "always"]
    }
}
